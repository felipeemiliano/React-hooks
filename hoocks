import { useState } from "react";

const App = () => {
  const [ cout, setCout ] = useState(0);

  const incrementCount = () => {
    setCout((prevState) => +1);
  };
  return (
    <div>
      <h1> {count}</h1>
      <button onclick={incrementCount}>Increment</button>
    </div>
  );
};


export default App;
